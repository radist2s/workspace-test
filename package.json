{
  "name": "@radist2s/workspace-test",
  "version": "1.0.11",
  "main": "index.js",
  "license": "MIT",
  "workspaces": [
    "packages/*"
  ],
  "type": "module",
  "scripts": {
    "build": "yarn workspace @radist2s/app run build",
    "release": "yarn workspace @radist2s/app run build && changeset publish",
    "start": "node src/index.js",
    "prepare": "husky install"
  },
  "packageManager": "yarn@3.3.1",
  "devDependencies": {
    "auto": "^10.37.6",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "lerna": "^6.4.1",
    "prettier": "^2.8.3"
  },
  "repository": "https://github.com/radist2s/workspace-test",
  "author": "Alex <radist2s@gmail.com>",
  "auto": {
    "plugins": [
      [
        "npm",
        {
          "exact": true
        }
      ]
    ]
  },
  "dependencies": {
    "@changesets/cli": "^2.26.0"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
}
