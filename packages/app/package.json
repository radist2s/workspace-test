{
  "name": "@radist2s/app",
  "private": true,
  "version": "1.0.12",
  "WORKSPACE_PACKAGE_DIST_FILES_USAGE_PATCH": {
    "main": "./_dist/index.js",
    "exports": {
      ".": "./_dist/index.js",
      "./*": "./_dist/*",
      "./package.json": "./package.json"
    },
    "typesVersions": {
      ">=3.6": {
        "*": [
          "./_dist/*"
        ]
      }
    }
  },
  "scripts": {
    "dev": "yarn clean",
    "clean": "rimraf ./_dist && yarn clean:revert-patch-exports",
    "clean:revert-patch-exports": "node ./copyDistPackageJson.js --revert-dist-exports",
    "build": "tsc && yarn build:patch-exports",
    "build:patch-exports": "node ./copyDistPackageJson.js --create-dist --patch-dist-exports",
    "build:copy-docs": "cpy './*.md' '!_dist/*' _dist",
    "test": "vitest run"
  },
  "license": "MIT",
  "type": "module",
  "dependencies": {
    "cpy-cli": "^4.2.0",
    "rimraf": "^4.1.1",
    "typescript": "^4.9.4",
    "vitest": "^0.27.2"
  },
  "devDependencies": {
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1"
  }
}
